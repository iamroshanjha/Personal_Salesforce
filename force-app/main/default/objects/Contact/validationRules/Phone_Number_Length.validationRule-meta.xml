<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone_Number_Length</fullName>
    <active>false</active>
    <errorConditionFormula>OR(ISBLANK(Phone),LEN(Phone)&lt;10)</errorConditionFormula>
    <errorDisplayField>Phone</errorDisplayField>
    <errorMessage>Phone Number should be 10 digit</errorMessage>
</ValidationRule>
