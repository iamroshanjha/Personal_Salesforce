<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AnnualRevenue_for_Banking</fullName>
    <active>false</active>
    <errorConditionFormula>IF(ISPICKVAL(Industry,&quot;Banking&quot;) ,AnnualRevenue &lt;=0,NULL)</errorConditionFormula>
    <errorDisplayField>AnnualRevenue</errorDisplayField>
    <errorMessage>AnnualRevenue should not be null for Industry Banking</errorMessage>
</ValidationRule>
